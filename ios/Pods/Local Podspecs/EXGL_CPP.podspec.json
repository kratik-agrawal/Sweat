{
  "name": "EXGL_CPP",
  "version": "11.1.1",
  "summary": "C++ bindings for WebGL 2.0 used in Expo GL module",
  "description": "C++ bindings for WebGL 2.0 used in Expo GL module",
  "license": "MIT",
  "authors": {
    "name": "650 Industries, Inc."
  },
  "homepage": "https://docs.expo.dev/versions/latest/sdk/gl-view/",
  "platforms": {
    "ios": "12.0"
  },
  "source": {
    "git": "https://github.com/expo/expo.git"
  },
  "static_framework": true,
  "frameworks": "OpenGLES",
  "compiler_flags": "-x objective-c++ -std=c++1z -fno-aligned-allocation",
  "pod_target_xcconfig": {
    "CLANG_WARN_COMMA": "NO",
    "CLANG_WARN_UNGUARDED_AVAILABILITY": "NO",
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) GLES_SILENCE_DEPRECATION=1"
  },
  "dependencies": {
    "React-jsi": [

    ]
  },
  "source_files": "**/*.h",
  "vendored_frameworks": "EXGL_CPP.xcframework"
}
